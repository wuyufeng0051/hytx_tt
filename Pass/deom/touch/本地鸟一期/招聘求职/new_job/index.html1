    jQuery(document).ready(function () {

       
        $('.date-picker').datepicker({
            language: 'cn',
            autoclose: true,
            todayHighlight: true
        });

    $('body').click(function(){
        $('.data_box ul').hide();
    })
    $('.data_box span').click(function(){
        if ($('.data_box ul').css("display") == "none") {
            $('.data_box ul').show();
        }else{
            $('.data_box ul').hide();
        }
        return false
    })
    $('.data_box ul li').click(function(){
        var x = $(this),
            txt = x.text();
        $('.data_box span em').text(txt);
        $('.data_box ul').hide();
    })
    $('.peronal_set').click(function(){
        $('.data_choice').show();
    })
    $('.set_hide').click(function(){
        $('.data_choice').hide();
    })
    $('.biao_btn').click(function(){
        if ($('.biao_box1').css("display") == "none") {
            $('.biao_box1').show();
            $('.disk').show(); 
        }else{
            $('.biao_box1').hide();
            $('.disk').hide();
