<script type="text/javascript" src="media/js/bootstrap-datepicker.js"></script>

<!-- END PAGE LEVEL PLUGINS -->

<!-- BEGIN PAGE LEVEL SCRIPTS -->

<script src="media/js/app.js" type="text/javascript"></script>

<script src="media/js/index.js" type="text/javascript"></script>

<script src="media/js/form-components.js"></script>
<!-- END PAGE LEVEL SCRIPTS -->

<script  type="text/javascript" src="media/js/layer.js" ></script>

<script>

    jQuery(document).ready(function () {

        App.init(); // initlayout and core plugins
       
        $('.date-picker').datepicker({
            language: 'cn',
            autoclose: true,
            todayHighlight: true
        });

    $('body').click(function(){
        $('.data_box ul').hide();
    })
    $('.data_box span').click(function(){
        if ($('.data_box ul').css("display") == "none") {
            $('.data_box ul').show();
        }else{
            $('.data_box ul').hide();
        }
        return false
    })
    $('.data_box ul li').click(function(){
        var x = $(this),
            txt = x.text();
        $('.data_box span em').text(txt);
        $('.data_box ul').hide();
    })
    $('.peronal_set').click(function(){
        $('.data_choice').show();
    })
    $('.set_hide').click(function(){
        $('.data_choice').hide();
    })
    $('.biao_btn').click(function(){
        if ($('.biao_box1').css("display") == "none") {
            $('.biao_box1').show();
            $('.disk').show(); 
        }else{
            $('.biao_box1').hide();
            $('.disk').hide();
        }
    })
    $('.clo , .sure').click(function(){
        $('.biao_box1').hide();
        $('.disk').hide();
    })

    // 选中标签
   $('.biao_list').delegate(".tips","click",function(){
        var x = $(this);
        if (x.hasClass('bb_bc')) {
            x.removeClass('bb_bc');
        }else{
            x.addClass('bb_bc');
        }
    })
   // 添加标签盒子展开收起
   $('.biao_add').click(function(){
        if ($('.add_box').css("display") == "none") {
            $('.add_box').show();
            $('.addb_sure').show();
            $('.biao_add').text('收起');
        }else{
            $('.add_box').hide();
            $('.addb_sure').hide();
            $('.biao_add').text('添加标签');
        }
   })
   // 添加标签
    $('.addb_sure span').click(function(){
        var val = $('.add_box textarea').val().replace(/，/ig,',').split(',');
        var txt = "";
        for (var i = 0; i < val.length - 1; i++) {
            txt = "<span><i>"+ val[i] +"</i><em>+</em></span>"
            $('.biao_list').append(txt);
        }
    })
    // 添加活动盒子展开收起
    $('.active_choice').click(function(){
        if ($('.active_box').css("display") == "none") {
            $('.active_box').show();
            $('.active_choice').text('收起');
        }else{
            $('.active_box').hide();
            $('.active_choice').text('添加');
        }
    })
    // 删除标签
    $('.biao_edit').click(function(){
        if ($('.biao_list span').hasClass('del')) {
            $('.biao_list span').removeClass('del');
            $('.biao_list span').addClass('tips');
            $('.biao_list span em').text("+")
            $('.biao_edit').text('编辑标签');
        }else{
            $('.biao_list span').addClass('del');
            $('.biao_list span').removeClass('tips');
            $('.biao_list span em').text("×")
            $('.biao_edit').text('完成');
        }
    })
    $('.biao_list').delegate(".del","click",function(){
        var x = $(this);
        x.remove();
    })
    $('.clo').click(function(){
        $('.biao_box1').hide();
        $('.disk').hide();
    })
    $('.disk').click(function(){
        $('.biao_box1').hide();
        $('.disk').hide();
    })
    //询问框
    $('.shan_btn').click(function(){
        var x = $(this);
        layer.confirm('是否删除？', {
          btn: ['确定','取消'] //按钮
        }, function(){
          layer.msg('已删除', {icon: 1});
          x.closest('tr').remove();
        }, function(){
          
        });
    })
    });

</script>

<!-- END JAVASCRIPTS -->

</body>

<!-- END BODY -->

</html>
